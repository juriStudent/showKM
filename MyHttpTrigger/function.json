{
  "bindings": [
    {
      "type": "httpTrigger",
      "name": "req",
      "direction": "in",
      "methods": [
        "get",
        "post"
      ],
      "authLevel": "anonymous",
      "route": "{password}"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "databaseIN",
      "type": "cosmosDB",
      "databaseName": "juri_km",
      "collectionName": "km_reg",
      "connectionStringSetting": "connectionStringSetting",
      "sqlQuery": "SELECT CONCAT(c.surName, ' ',c.lastName) as name, c.vehicleCode, c.km, c.timeID, c.vehicleDescription from c where c.status = 'submitted' ORDER BY c.timeID DESC",
      "direction": "in"
    },
    {
      "name": "workersOUT",
      "type": "cosmosDB",
      "databaseName": "juri_km",
      "collectionName": "km_reg",
      "createIfNotExists": true,
      "connectionStringSetting": "connectionStringSetting",
      "direction": "out"
    }
  ]
}